{
    "command-line": "[INPUT_STR] [INPUT_LIST] [INPUT_FILE] [INPUT_NUM]", 
    "description": "A command-line with characters surrounding input values", 
    "inputs": [
        {
            "id": "param_str", 
            "name": "String Input", 
            "type": "String", 
            "value-key": "[INPUT_STR]",
            "command-line-flag": "-s",
            "surrounding-char": "\""
        },
        {
            "id": "param_list", 
            "name": "List Input", 
            "type": "String",
            "list": true,
            "value-key": "[INPUT_LIST]",
            "surrounding-char": "."
        },
        {
            "id": "param_file", 
            "name": "File Input", 
            "type": "File",
            "value-key": "[INPUT_FILE]",
            "surrounding-char": "%"
        },
        {
            "id": "param_num", 
            "name": "Number Input", 
            "type": "Number",
            "value-key": "[INPUT_NUM]",
            "surrounding-char": "*"
        }
    ],
    "name": "Surrounding Characters Example", 
    "output-files": [
        {
            "id": "output_file", 
            "name": "output file", 
            "path-template": "file.txt"
        }
    ], 
    "schema-version": "0.5", 
    "tool-version": "0.1"
}
